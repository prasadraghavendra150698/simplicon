    {
      "source": "/portal.html",
      "destination": "/portal",
      "permanent": true
    },
    {
      "source": "/admin.html",
      "destination": "/admin",
      "permanent": true
    },
    {
      "source": "/admin-requests.html",
      "destination": "/admin-requests",
      "permanent": true
