{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/pricing",
      "destination": "/pricing.html"
    },
    {
      "source": "/resources",
      "destination": "/resources.html"
    },
    {
      "source": "/auth",
      "destination": "/auth.html"
    },
    {
      "source": "/portal",
      "destination": "/portal.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/admin-requests",
      "destination": "/admin-requests.html"
    },
    {
      "source": "/admin-ticket",
      "destination": "/admin-ticket.html"
    },
    {
      "source": "/admin-access",
      "destination": "/admin-access.html"
    },
    {
      "source": "/admin-members",
      "destination": "/admin-members.html"
